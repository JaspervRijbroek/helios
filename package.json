{
    "name": "@jaspervrijbroek/helios",
    "description": "A need for speed world server emulator",
    "version": "1.0.0",
    "dependencies": {
        "@prisma/client": "^2.25.0",
        "@types/bcrypt": "^5.0.0",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.12",
        "@types/glob": "^7.1.3",
        "@types/node": "^14.14.19",
        "@types/uuid": "^8.3.0",
        "@types/xml2json": "^0.11.3",
        "@types/xmpp__jid": "^1.3.1",
        "@types/xmpp__xml": "^0.6.1",
        "@xmpp/jid": "^0.12.0",
        "@xmpp/xml": "^0.12.0",
        "bcrypt": "^5.0.1",
        "chalk": "^4.1.1",
        "debug": "^4.3.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "glob": "^7.1.7",
        "js2xmlparser": "^4.0.1",
        "prisma": "^2.25.0",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.2",
        "uuid": "^8.3.2",
        "xml2json": "^0.12.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.7"
    },
    "scripts": {
        "start": "ts-node --files index.ts",
        "dev": "nodemon",
        "test": "jest"
    },
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            ".git",
            "node_modules"
        ],
        "env": {
            "DEBUG": "nfsw:*"
        },
        "exec": "./node_modules/.bin/ts-node --files index.ts",
        "watch": [
            "./src/**/*.ts",
            "./index.ts"
        ],
        "ext": "ts"
    }
}
